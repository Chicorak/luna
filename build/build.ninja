 description = $DESC
 restat = 1

rule REGENERATE_BUILD
 command = /usr/lib/python-exec/python3.8/meson --internal regenerate /home/abbix/Documents/Projects/luna/src /home/abbix/Documents/Projects/luna/build --backend ninja
 description = Regenerating build files.
