# Phony build target, always out of date

build PHONY: phony 

# Build rules for targets

build luna.p/luna.c.o: c_COMPILER ../src/luna.c
 DEPFILE = luna.p/luna.c.o.d
 DEPFILE_UNQUOTED = luna.p/luna.c.o.d
 ARGS = -Iluna.p -I. -I../src -fdiagnostics-color=always -pipe -D_FILE_OFFSET_BITS=64 -Wall -Winvalid-pch -g -ansi -Os

build luna.p/main.c.o: c_COMPILER ../src/main.c
